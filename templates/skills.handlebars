<div id="skills">
	<div class="profile-title title">Technical Skills</div>

	{{#each skills}}
	<div class="skill">
		<span class="skill-name title">{{this.name}}</span>
		<div class="skill-value">
			<div class="skill-value-bar highlight-background" style="width:{{this.value}}0%;"></div>
		</div>
	</div>
	{{/each}}
</div>
